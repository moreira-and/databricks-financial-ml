[flake8]
max-line-length = 100
extend-ignore = E203, W503, F401
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info

per-file-ignores =
    __init__.py:F401
    transformacoes/bronze.py:F821
    transformacoes/prata.py:F821
    transformacoes/ouro.py:F821
    utilitarios/fontes_dados.py:F821
